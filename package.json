  "repository": {
    "type": "git",
    "url": "git@github.com:jenkinsci/pipeline-graph-view-plugin.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jenkinsci/pipeline-graph-view-plugin/issues"
  },
  "homepage": "https://github.com/jenkinsci/pipeline-graph-view-plugin/#readme",
  "dependencies": {
    "@mui/icons-material": "5.10.9",
    "@mui/lab": "5.0.0-alpha.104",
    "@mui/material": "5.10.10",
    "@react-spring/web": "9.5.4",
    "@reacticons/ionicons": "6.0.4",
    "html-react-parser": "1.4.14",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/enzyme": "3.10.12",
    "@types/jest": "26.0.24",
    "@types/node": "16.18.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "css-loader": "3.6.0",
    "import": "^0.0.6",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "prettier": "2.7.1",
    "react-collapse-pane": "^3.0.1",
    "sass": "1.55.0",
    "sass-loader": "13.1.0",
    "source-map-loader": "0.2.4",
    "style-loader": "^1.3.0",
    "styled-components": "^5.3.6",
    "ts-jest": "27.0.4",
    "ts-loader": "^7.0.5",
    "typescript": "4.1.5",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
